{
  "name": "ariel_website",
  "version": "1.0.0",
  "description": "template for personal website",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch",
    "build": "set NODE_ENV=production webpack --progress --colors -p --config webpack.prod.config.js",
    "dev": "webpack-dev-server --progress --colors --watch",
    "start": "nodemon server.js"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "collectCoverageFrom": [
      "*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asalem1/Personal_Site.git"
  },
  "author": "Ariel Salem",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Asalem1/Personal_Site/issues"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "homepage": "https://github.com/Asalem1/Personal_Site#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-beta",
    "ejs": "^2.5.6",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "google-auth-library": "^0.10.0",
    "googleapis": "^20.1.0",
    "jquery": "^3.2.1",
    "mailgun-js": "^0.13.1",
    "material-ui": "^0.19.4",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-coverflow": "^0.2.3",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "react-slick": "^0.15.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.12",
    "reactstrap": "^4.8.0",
    "slick-carousel": "^1.8.1",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-mixins": "^6.2.0",
    "react-hot-loader": "^3.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.20.0"
  }
